{
  "name": "pac-shield-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/pac-shield-api/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack-cli build",
        "args": [
          "--node-env=production"
        ],
        "cwd": "apps/pac-shield-api"
      },
      "configurations": {
        "development": {
          "args": [
            "--node-env=development"
          ]
        }
      }
    },
    "prune-lockfile": {
      "dependsOn": [
        "build"
      ],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/pac-shield-api/package.json",
        "{workspaceRoot}/dist/apps/pac-shield-api/yarn.lock"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": [
        "build"
      ],
      "cache": true,
      "outputs": [
        "{workspaceRoot}/dist/apps/pac-shield-api/workspace_modules"
      ],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": [
        "prune-lockfile",
        "copy-workspace-modules"
      ],
      "executor": "nx:noop"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": [
        "build"
      ],
      "options": {
        "buildTarget": "pac-shield-api:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "pac-shield-api:build:development"
        },
        "production": {
          "buildTarget": "pac-shield-api:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/pac-shield-api/jest.config.ts",
        "passWithNoTests": true
      }
    },

    "prisma-format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma format --schema src/prisma/schema.prisma",
        "cwd": "apps/pac-shield-api"
      }
    },

    "prisma-validate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma validate --schema src/prisma/schema.prisma",
        "cwd": "apps/pac-shield-api"
      }
    },

    "prisma-generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate --schema src/prisma/schema.prisma",
        "cwd": "apps/pac-shield-api"
      }
    },

    "prisma-db-push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma db push --schema src/prisma/schema.prisma",
        "cwd": "apps/pac-shield-api"
      }
    },

    "prisma-studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma studio --schema src/prisma/schema.prisma",
        "cwd": "apps/pac-shield-api"
      }
    },

    "prisma-all": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma format --schema src/prisma/schema.prisma && npx prisma validate --schema src/prisma/schema.prisma && npx prisma generate --schema src/prisma/schema.prisma",
        "cwd": "apps/pac-shield-api"
      }
    }
  }
}
